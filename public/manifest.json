{
  "manifest_version": 2,

  "name": "Baraholka search optimizer",
  "description": "Baraholka search optimizer",
  "version": "2.0",

  "content_scripts": [
    {
      "matches": ["https://baraholka.onliner.by/*"],
      "js": ["js/app.js", "js/chunk-vendors.js"]
